import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter a 4-digit year: ");
        int year = sc.nextInt();

        if (year < 1000 || year > 9999) {
            System.out.println("Please enter a valid 4-digit year.");
        } else {
            int firstTwo = year / 100;
            int lastTwo = year % 100;
            if (firstTwo == lastTwo) {
                System.out.println("The first two digits and last two digits are equal.");
            } else {
                System.out.println("The first two digits and last two digits are not equal.");
            }
        }

        sc.close();
    }
}
